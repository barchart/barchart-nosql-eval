
#
# opscenter-agent network configuation.
#


# Private internal interface for agent local socket binding.
# Same as listen_address in cassandra.yaml
agent_rpc_interface: <%= @agent_rpc_interface %>

# Public external host name or address for inbound nat which maps into agent_rpc_interface.
# Same as broadcast_address in cassandra.yaml
agent_rpc_broadcast_address: <%= @agent_rpc_broadcast_address %>

#
#
#

# Public external host name or addrss for inbound nat. (https)
# Same as broadcast_address in cassandra.yaml
local_interface: <%= @local_interface %>        


# Public external host name or addrss for opscenter master.
# 
stomp_interface: <%= @stomp_interface %>

